{
  "name": "frontend",
  "private": true,
  "version": "0.1.0",
  "description": "Threejs frontend which employs the trained policy for balancing bot",
  "author": "Eren Cetin <erencetin98@gmail.com>",
  "license": "MIT",
  "type": "module",
  "keywords": ["threejs", "typescript", "3d", "webgl"],
  "repository": {
    "type": "git",
    "url": "https://github.com/erenovic/frontend.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist",
    "ci": "npm run type-check && npm run lint && npm run test && npm run build"
  },
  "devDependencies": {
    "vite": "^7.1.7",
    "typescript": "^5.5.3",
    "@types/three": "^0.168.0",
    "gsplat": "^1.2.8",
    "@types/node": "^22.0.0",
    "@biomejs/biome": "^1.8.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "vite-plugin-dts": "^4.0.0"
  },
  "dependencies": {
    "three": "^0.168.0",
    "gsplat": "^1.2.8"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "biome check --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
